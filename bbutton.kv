MDBoxLayout:
    orientation: 'vertical'
    MDTopAppBar:
        title: "Out Top Toolbar"
        left_action_items: [["menu"]]
        right_action_items: [["dots-vertical"]]
    MDLabel:
        id: my_label
        text: "Some Stuff"
        halign: "center"
    MDBottomAppBar:
        MDTopAppBar:
            title:"Bottom Menu"
            left_action_items: [["menu",lambda x:app.presser()]]
            icon: "contacts"
            type: "bottom"
            mode: "end"
            #Click the Button
            on_action_button:app.presser()